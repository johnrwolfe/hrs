INSERT INTO SPR_PS
	VALUES ("74f96f34-1006-427c-994b-b3cd707bc2b6",
	'CreateNewAccount',
	'',
	'
select many account from instances of Account where selected.EmployeeID==param.EmployeeID;// or selected.Username==::getUserName( F:param.First_Name, L:param.Last_Name);
LOG::LogInfo(message:"Account: attempting to set username and passowrd for "+ param.First_Name+" "+param.Last_Name);	

if ( empty account)
	create object instance acc of Account;
	acc.EmployeeID = param.EmployeeID;
	acc.Username = ::getUserName( F:param.First_Name, L:param.Last_Name);
	acc.Password = ::getPassword( EmployeeID:param.EmployeeID, FirstLastName:param.First_Name+param.Last_Name, CurrentSeconds:TIM::current_seconds());
	
	select any group from instances of Group where selected.Name=="Employees";
	relate acc to group across R1.''a member of'';

LOG::LogInfo(message:"Account: account created for employee "+ param.First_Name+" "+param.Last_Name+" Username: "+acc.Username+ " Group "+ group.Name);	
else
	LOG::LogInfo(message:"Account: account set is not empty ");	

end if;


',
	1,
	0);
